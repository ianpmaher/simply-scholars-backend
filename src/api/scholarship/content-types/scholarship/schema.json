{
  "kind": "collectionType",
  "collectionName": "scholarships",
  "info": {
    "singularName": "scholarship",
    "pluralName": "scholarships",
    "displayName": "scholarships",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "isActive": {
      "type": "boolean",
      "default": true,
      "required": false
    },
    "value": {
      "type": "integer"
    },
    "pdf": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "files"
      ]
    },
    "slug": {
      "type": "uid"
    },
    "eligibility": {
      "type": "enumeration",
      "enum": [
        "Merit-Based",
        "Needs-Based",
        "Merit- & Needs-Based",
        "Other"
      ]
    },
    "hasPic": {
      "type": "boolean",
      "default": false
    },
    "hasPdf": {
      "type": "boolean"
    },
    "scholarshipContent": {
      "type": "dynamiczone",
      "components": [
        "scholarship-form.title",
        "scholarship-form.description",
        "scholarship-form.pic",
        "scholarship-form.background",
        "scholarship-form.deadline"
      ]
    },
    "name": {
      "type": "string",
      "required": true,
      "default": "name"
    }
  }
}
